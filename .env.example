# Configuración del dominio - CAMBIAR POR TU DOMINIO
DOMAIN=tudominio.com
N8N_HOST=n8n.tudominio.com
TRAEFIK_HOST=traefik.tudominio.com
WEBHOOK_URL=https://n8n.tudominio.com/

# Configuración de N8N
N8N_PROTOCOL=https
GENERIC_TIMEZONE=America/Mexico_City

# Autenticación N8N - CAMBIAR CREDENCIALES
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin123

# Base de datos PostgreSQL - CAMBIAR CONTRASEÑA
POSTGRES_DB=n8n
POSTGRES_USER=n8n_user
POSTGRES_PASSWORD=postgres123

# Configuración de base de datos para N8N
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=postgres
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=n8n_user
DB_POSTGRESDB_PASSWORD=postgres123

# Clave de encriptación - SE GENERA AUTOMÁTICAMENTE
N8N_ENCRYPTION_KEY=

# Email para certificados SSL - CAMBIAR POR TU EMAIL
LETSENCRYPT_EMAIL=admin@tudominio.com